import { renderHook, act, waitFor } from '@testing-library/react';
import { useProducts } from './useProducts';

// Mock API
jest.mock('@/lib/api', () => ({
  fetchProducts: jest.fn(),
}));

describe('useProducts', () => {
  it('should fetch products on mount', async () => {
    const mockProducts = [{ id: '1', name: 'Product 1' }];
    (fetchProducts as jest.Mock).mockResolvedValue(mockProducts);
    
    const { result } = renderHook(() => useProducts());
    
    // ĞĞ°Ñ‡Ğ°Ğ»ÑŒĞ½Ğ¾Ğµ ÑĞ¾ÑÑ‚Ğ¾ÑĞ½Ğ¸Ğµ
    expect(result.current.isLoading).toBe(true);
    expect(result.current.data).toBeUndefined();
    
    // ĞŸĞ¾ÑĞ»Ğµ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸
    await waitFor(() => {
      expect(result.current.isLoading).toBe(false);
    });
    
    expect(result.current.data).toEqual(mockProducts);
  });
  
  it('should handle error', async () => {
    const error = new Error('Failed to fetch');
    (fetchProducts as jest.Mock).mockRejectedValue(error);
    
    const { result } = renderHook(() => useProducts());
    
    await waitFor(() => {
      expect(result.current.isLoading).toBe(false);
    });
    
    expect(result.current.error).toBe(error);
  });
  
  it('should refetch when filters change', async () => {
    const { result, rerender } = renderHook(
      ({ filters }) => useProducts(filters),
      { initialProps: { filters: { category: 'a' } } }
    );
    
    await waitFor(() => {
      expect(result.current.isLoading).toBe(false);
    });
    
    // Ğ˜Ğ·Ğ¼ĞµĞ½ÑĞµĞ¼ Ñ„Ğ¸Ğ»ÑŒÑ‚Ñ€Ñ‹
    rerender({ filters: { category: 'b' } });
    
    expect(fetchProducts).toHaveBeenCalledWith({ category: 'b' });
  });
});
